html, body {
    margin: 0;
    background: #262626;
}
ul {
    list-style: none;
    padding-left: 0;
}
.hidden {
    display: none;
}
.close {
    position: absolute;
    top: 0;
    right: 6px;
    color: #999;
    font-size: 18px;
    padding: 12px;
}
.close:hover {
    color: #FFF;
    cursor: pointer;
}

.skee {
    min-width: 640px;
    font-family: Lucida Grande,Lucida Sans Unicode,Arial,sans-serif;
    color: #FFF;

    .header {
        padding: 8px 8px 4px 8px;
        text-align: center;
        border-top: 2px solid #FFF;
        border-bottom: 1px solid #FFF;

        .settingsContainer {
            float: left;
        }
        .title {
            display: inline-block;
        }
        .titleText {
            background-image: url($img + '/title4.png');
            width: 188px;
            height: 44px;
            display: inline-block;
        }
    }

    .main {
        margin-top: 1px;

        .menu {
            float: left;
            width: 240px;
            height: 618px;
            border-right: 1px solid #FFF;
            color: #FFF;

            .suggestions {
                border-bottom: 1px solid #FFF;
            }

            .addSong {
                border-bottom: 1px solid #FFF;
            }
        }

        .playerContainer {
            margin-left: 240px;
        }
    }
}

.subtitlesUploadContainer {
    display: none;
    font-size: 14px;
    line-height: 24px;
    color: #FFF;
    background-color: #444;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 2;
    overflow: hidden;
}
.subtitlesUpload {
    margin: 10px;
}
.dictionaryContainer {
    display: none;
    font-size: 14px;
    line-height: 24px;
    color: #FFF;
    background-color: #444;
    position: absolute;
    top: 0;
    left: 0;
    width: 50%;
    min-height: 200px;
    z-index: 2;
    margin: 32px;
    border: 1px solid #000;
    border-radius: 5px;
    box-shadow: 1px 1px 10px #000;
    overflow: hidden;
}
.dictionaryContainer p {
    padding: 0;
    margin: 0;
}
.dictionaryResults {
    margin-top: 48px;
    padding: 0 32px 32px 32px;
    max-height: 400px;
    overflow: auto;
}