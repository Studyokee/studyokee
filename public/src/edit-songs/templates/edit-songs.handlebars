<div class="col-lg-12">
    <div class="table-responsive">
        <table class="table table-striped table-bordered table-hover">
            <thead>
                <tr>
                    <td>
                        Open
                    </td>
                    <td>
                        Remove
                    </td>
                    <td>
                        Track Name
                    </td>
                    <td>
                        Artist
                    </td>
                    <td>
                        Youtube Key
                    </td>
                    <td>
                        Rdio Key
                    </td>
                    <td>
                        Language
                    </td>
                    <td>
                        Subtitles
                    </td>
                    <td>
                        Translations
                    </td>
                </tr>
            </thead>
            <tbody>
            {{#each data}}
                <tr>
                    <td>
                        <button class="open btn btn-primary btn-xs" data-index="{{@index}}">Open</button>
                    </td>
                    <td>
                        <button class="remove btn btn-primary btn-xs" data-index="{{@index}}">Remove</button>
                    </td>
                    <td>
                        {{metadata.trackName}}
                    </td>
                    <td>
                        {{metadata.artist}}
                    </td>
                    <td>
                        {{youtubeKey}}
                    </td>
                    <td>
                        {{rdioKey}}
                    </td>
                    <td>
                        {{metadata.language}}
                    </td>
                    <td>
                        {{subtitles.length}}
                    </td>
                    <td>
                        {{translations.length}}
                    </td>
                </tr>
            {{/each}}
            </tbody>
        </table>
    </div>
</div>