.spinnerContainer {
    height: 100%;
    width: 100%;
}
.spinner {
    height: 30px;
    width: 30px;
    background-image: url($img + '/loading.png');
    -webkit-animation:spin 0.6s infinite steps(11);
    margin-left: auto;
    margin-right: auto;
    margin-top: 16px;
    margin-bottom: 16px;
}
@-webkit-keyframes spin {
    from {background-position:0px 0;}
    to {background-position:-330px 0;}
}