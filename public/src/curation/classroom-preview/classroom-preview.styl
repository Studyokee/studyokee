.classroom-preview {
    cursor: pointer;
    
    .panel {
        height: 250px;
    }

    a.classroomLink {
        height: 100%;
        border-radius: 0;
        overflow: hidden;
        height: 100%;
        width: 100%;
        display: inline-block;
        cursor: pointer;
    }
    .hasMoreItems {
        font-size: 24px;
        margin-top: -18px;
    }
    h5 {
        margin: 0;
        padding-top: 16px;
    }
}
